generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model youtube_videos {
  id     String    @default(cuid()) @id
  title  String  @db.VarChar(255)
  url    String  @db.VarChar(255)
  upvote Int?
  posted String? @db.VarChar(255)
  downvote Int?
}
